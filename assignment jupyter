import random 

def play_game(target_number, player_name):
    num_tries = 0
    while True:
        num_tries += 1
        guess = int(input(f"{player_name}: "))
        if guess > target_number:
            print("Try a smaller number.")
        elif guess < target_number:
            print("Try a greater number.")
        else:
            print(f"That's right! number of tries: {num_tries}")
            return num_tries
target_number = random.randint(0, 10)
num_tries_player1 = play_game(target_number, "player 1")
num_tries_player2 = play_game(target_number, "player 2")
if num_tries_player1 < num_tries_player2:
    print("Winner is player 1")
elif num_tries_player1 > num_tries_player2:
    print("Winner is player 2")
else:
    print("It's a draw!")
